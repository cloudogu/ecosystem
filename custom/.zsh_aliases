# backup
alias bbackup="cd /vagrant/containers/backup && cesapp build ."
alias lbackup="tail -f /var/log/docker/backup.log"
alias llbackup="cat -n /var/log/docker/backup.log"
alias tbackup="truncate -s 0 /var/log/docker/backup.log"
alias ebackup="docker exec -it backup bash"

