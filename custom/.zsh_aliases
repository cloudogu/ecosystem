# cas
alias bcas="cd /vagrant/containers/cas && cesapp build ."
alias lcas="tail -f /var/log/docker/cas.log"
alias clcas="cat -n /var/log/docker/cas.log"
alias tlcas="truncate -s 0 /var/log/docker/cas.log"
alias ecas="docker exec -it cas bash"
alias icas="cesapp install official/cas"
alias ccas="cesapp edit-config cas"
alias sccas="etcdctl -output json ls -r /config/cas | jq"
alias rcas="docker restart cas"
alias gcas="cd /vagrant/containers/cas"
alias pcas="cesapp purge cas"

